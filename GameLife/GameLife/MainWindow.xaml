<Window x:Class="GameLife.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameLife"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="500">
    <Grid x:Name="mainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="40"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Name="btnStart" Content="Start" Width="80" Height="20" Margin="-100,0,0,0" Click="btnStart_Click"/>
        <Button Name="btnStop" Content="Stop" Width="80" Height="20" Margin="100,0,0,0" Click="btnStop_Click"/>
        <Grid Grid.Row="1" Name="grid1">
        </Grid>
    </Grid>
</Window>
